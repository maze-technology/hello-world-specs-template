{
  "version": "1.0",
  "sources": ["src/main/smithy"],
  "maven": {
    "dependencies": [
      "com.disneystreaming.alloy:alloy-core:0.3.36",
      "tech.maze:smithy-temporal-traits:0.0.1"
    ],
    "repositories": [
      {
        "url": "https://maven.pkg.github.com/maze-technology/smithy-temporal-traits",
        "httpCredentials": "${GITHUB_USERNAME}:${GITHUB_TOKEN}"
      },
      {
        "url": "https://repo.maven.apache.org/maven2/"
      }
    ]
  },
  "projections": {
    "resolved": {
      "plugins": {
        "sources": {
          "format": "smithy",
          "outputDirectory": "model"
        }
      }
    }
  }
}
